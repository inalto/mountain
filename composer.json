{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "akaunting/laravel-module": "^2.0",
        "astrotomic/laravel-translatable": "^11.9",
        "bugsnag/bugsnag-laravel": "^2.21",
        "codezero/laravel-localized-routes": "^2.4",
        "cviebrock/eloquent-sluggable": "^9.0",
        "doctrine/dbal": "^3.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.2",
        "inertiajs/inertia-laravel": "^0.2.4",
        "kalnoy/nestedset": "^6.0",
        "laravel/dusk": "^6.11",
        "laravel/framework": "^9.0",
        "laravel/jetstream": "^2.0",
        "laravel/sanctum": "^2.6",
        "laravel/scout": "^9.4",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "laraveldaily/laravel-charts": "^0.1.24",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "maxeckel/livewire-editorjs": "^1.1",
        "mcamara/laravel-localization": "^1.7",
        "mediconesystems/livewire-datatables": "^0.9",
        "meilisearch/meilisearch-php": "^0.24.2",
        "sibyx/phpgpx": "^1.1",
        "spatie/laravel-medialibrary": "^10.1",
        "spatie/laravel-medialibrary-pro": "dev-main",
        "spatie/laravel-ray": "^1.3",
        "spatie/laravel-site-search": "^1.0.0",
        "spatie/laravel-tags": "^4.3",
        "tightenco/ziggy": "^0.9.4",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.1",
        "laravel/pint": "^0.2.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.3.3",
        "spatie/laravel-ignition": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.spatie.be"
        }
    ]
}
